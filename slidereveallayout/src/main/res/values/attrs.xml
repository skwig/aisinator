<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SlideRevealLayout">
        <attr name="dragEdge">
            <flag name="left" value="1" />
            <flag name="right" value="2" />
        </attr>

        <attr name="mode">
            <enum name="normal" value="0" />
            <enum name="same_level" value="1" />
        </attr>

        <attr name="flingOpenVelocity" format="integer" />
        <attr name="flingDismissVelocity" format="integer" />

    </declare-styleable>
</resources>